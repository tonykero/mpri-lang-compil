function sum(arr, size) {
    var i;
    var sum;
    sum = 0;
    for(i = 0, i < size, i = i+1) {
        sum = sum + arr[i];
    }
    return sum;
}

function sum2d(arr, size1, size2) {
    var i;
    var sum;
    var tmp;
    sum = 0;
    for(i = 0, i < size1, i = i+1) {
        sum = sum + sum(arr[i], size2);
    }
    return sum;
}

